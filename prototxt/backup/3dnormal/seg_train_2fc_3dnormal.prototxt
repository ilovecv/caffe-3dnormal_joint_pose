  bottom: "fc6_55"
  top: "fc6_55"
}

layers {
  name: "fc8_3dnorm"
  type: INNER_PRODUCT
  bottom: "fc6_55"
  top: "fc8_3dnorm"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
    num_output: 8000
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layers {
  name: "loss_3dnorm"
